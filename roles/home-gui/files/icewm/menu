prog "Terminal" xterm xterm
prog "Midnight Commander" file xterm -geometry 120x40 -e mc -u
menu "Terminals" folder {
    prog "XTerm (80x10)" xterm xterm -geometry 80x10
    prog "XTerm (80x24)" xterm xterm -geometry 80x24
    prog "XTerm (120x40)" xterm xterm -geometry 120x40
}
separator
includeprog icewm-menu-fdo --no-sub-cats
separator
prog "!!! XKill !!!" xkill xkill
