[pull]
	rebase = true
[push]
	default = simple
[color]
	ui = auto
[credential]
	helper = store
[gui]
	encoding = utf-8
[guitool "Rebase"]
	cmd = rebase
[user]
	name = Anthony Perkins
	email = anthony@acperkins.com
[alias]
	ci = commit
	co = checkout
	ff = merge --ff-only
	graph = log --graph --oneline --all
	l1 = log --pretty=oneline
	last = log -1 HEAD --pretty=fuller
	lastmail = format-patch --stdout -1 HEAD
	unstage = reset HEAD --
	up = pull --rebase
