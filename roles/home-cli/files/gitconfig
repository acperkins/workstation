[core]
	autocrlf = input
[pull]
	ff = only
[push]
	default = simple
[color]
	ui = auto
[init]
	defaultBranch = main
[credential]
	helper = cache
[gui]
	encoding = utf-8
[guitool "Rebase"]
	cmd = rebase
[alias]
	graph = log --graph --oneline --all
	l1 = log --pretty=oneline
	last = log -1 HEAD --pretty=fuller
	lastmail = format-patch --stdout -1 HEAD
[include]
	path = .gitconfig.local
